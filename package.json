{
  "type": "commonjs",
  "name": "retro_stand_api",
  "version": "1.0.0",
  "description": "Retro Stand Api",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "start:dist": "node dist/index.js",
    "build": "tsc",
    "eslint": "eslint --fix .",
    "mgr:create": "typeorm migration:create src/migrations/%npm_config_name% -- -d",
    "mgr:run": "ts-node ./node_modules/typeorm/cli.js migration:run --dataSource=./src/shared/infrastructure/persistence/migration.config.ts"
  },
  "keywords": [],
  "author": "Manuel Rivera; Jose Fuilco",
  "license": "ISC",
  "dependencies": {
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
